      </Button>
      <Button fx:id="backAddEmpoloy" mnemonicParsing="false" onAction="#OnActionBackAddEmploy" text="Powrot">
         <StackPane.margin>
            <Insets left="300.0" top="250.0" />
         </StackPane.margin>
      </Button>
      <TextField fx:id="emailEmploy" alignment="CENTER" maxWidth="150.0" promptText="Email">
         <StackPane.margin>
            <Insets bottom="75.0" />
         </StackPane.margin>
      </TextField>
      <TextField fx:id="nameEmploy" alignment="CENTER" maxWidth="150.0" promptText="Nazwa">
         <StackPane.margin>
            <Insets bottom="225.0" />
         </StackPane.margin>
      </TextField>
      <TextField fx:id="passwordEmploy" alignment="CENTER" maxWidth="150.0" promptText="Password">
         <StackPane.margin>
            <Insets bottom="150.0" />
         </StackPane.margin>
      </TextField>
      <MenuButton fx:id="storeEmploy" mnemonicParsing="false" text="Sklep" />
